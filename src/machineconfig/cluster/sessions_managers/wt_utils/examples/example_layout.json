{
    "$schema": "https://bit.ly/cfglayout",
    "version": "0.1",
    "layouts": [
        {
            "layoutName": "g1",
            "layoutTabs": [
                {
                    "command": "yazi",
                    "startDir": "$HOME",
                    "tabName": "fileManager",
                    "tabWeight": 0
                },
                {
                    "command": "wsl",
                    "startDir": "$HOME",
                    "tabName": "wsl",
                    "tabWeight": 0
                },
                {
                    "command": "pwsh",
                    "startDir": "$HOME",
                    "tabName": "windows",
                    "tabWeight": 0
                }
            ]
        },
        {
            "layoutName": "whatever",
            "layoutTabs": [
                {
                    "command": "ls",
                    "startDir": "$HOME",
                    "tabName": "listDirTab",
                    "tabWeight": 0
                },
                {
                    "command": "pwsh",
                    "startDir": "$HOME",
                    "tabName": "windowsTab",
                    "tabWeight": 0
                }
            ]
        }
    ]
}
